<?xml version="1.0" encoding="UTF-8"?>
<command name="h.gc" desc="Geomorphic Classes">
	<field type="rastermap" required="true" name="slope" desc="slope" order="0" repr=" --igrass-slope #" />
	<field type="rastermap" required="true" name="net" desc="net" order="1" repr=" --igrass-net #" />
	<field type="rastermap" required="true" name="cp9" desc="cp9" order="2" repr=" --igrass-cp9 #" />
	<field type="double" required="true" name="thgrad" desc="thgrad" order="3" repr=" --thgrad #" />
	<field type="string" required="true" name="class" desc="class" order="4" repr=" --ograss-class #" />
	<field type="string" required="true" name="aggclass" desc="aggclass" order="5" repr=" --ograss-aggclass #" />
	<i18n key="slope" en="input slope map" it="mappa delle pendenze in ingresso"/>
	<i18n key="net" en="input stream network map" it="mappa del reticolo idrografico in ingresso"/>
	<i18n key="cp9" en="input map with 9 topological classes" it="mappa con la suddivisione in 9 classi topologiche in ingresso"/>
	<i18n key="class" en="output map with 11 geomorphic classes" it="mappa con la suddivisione in 11 classi in uscita"/>
	<i18n key="aggclass" en="output map with aggregated classes" it="mappa con le classi aggregate in uscita"/>
	<i18n key="thgrad" en="threshold on slope" it="soglia sulla pendenza"/>
</command>